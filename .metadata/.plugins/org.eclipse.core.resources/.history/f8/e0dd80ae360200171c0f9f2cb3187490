
public abstract class Pizza {
	
	String description = "Unknown";
	boolean carryout = false;
	final static int SMALL = 1;
	final int MEDIUM = 2;
	final int LARGE = 3;
	final int XLARGE = 4;
	int size = 0;
	
	public String getDescription(){
		
		return description;
		
	}
	public boolean getCarryout(){
		
		return carryout;
		
	}
	
	public abstract double cost();
	
	public int getSize() {
		// TODO Auto-generated method stub
		return size;
		
	}

}
